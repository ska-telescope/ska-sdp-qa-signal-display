{
  "name": "ska-spd-qa-display",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --reporters=default --reporters=jest-junit",
    "test:coverage": "react-scripts test --coverage --coverageDirectory=build/coverage",
    "code-analysis": "./node_modules/.bin/eslint \"src/**\" -f junit -o linting.xml",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@popperjs/core": "^2.11.2",
    "autoprefixer": "^10.4.2",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.5.0",
    "d3": "^7.3.0",
    "d3-path": "^3.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "i18next": "^21.6.6",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "jest": "^27.5.1",
    "jest-enzyme": "^7.0.1",
    "jest-junit": "^13.0.0",
    "junit": "^1.4.9",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "postcss": "^8.4.6",
    "prop-types": "^15.8.1",
    "react": "^16.13.0",
    "react-app": "^1.1.2",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^16.13.0",
    "react-i18next": "^11.15.3",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "sass": "^1.49.4",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "react-test-renderer": "^16.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/{!(i18n),}.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/index.jsx"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!d3|d3-array|internmap|delaunator|robust-predicates)"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
