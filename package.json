{
  "name": "qa-display",
  "version": "0.1.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "prettify": "prettier src/**/*.{ts,tsx} --write",
    "test": "react-scripts test --env=jsdom --reporters=default --reporters=jest-junit --passWithNoTests",
    "test:coverage": "react-scripts test --coverage --coverageDirectory=build/coverage --passWithNoTests",
    "code-analysis": "eslint src/** -f junit -o linting.xml"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/index.tsx"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!d3|d3-array|internmap|delaunator|robust-predicates)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.17.5",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "5.0.0-alpha.34",
    "@material-ui/icons": "5.0.0-alpha.34",
    "@material-ui/lab": "5.0.0-alpha.34",
    "@material-ui/system": "5.0.0-alpha.34",
    "@mui/base": "^5.0.0-alpha.69",
    "@mui/icons-material": "^5.4.2",
    "@mui/lab": "^5.0.0-alpha.70",
    "@mui/material": "^5.4.3",
    "@mui/styles": "^5.4.2",
    "@types/lodash": "^4.14.172",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.19",
    "@types/react-beautiful-dnd": "^13.1.1",
    "@types/react-dom": "^17.0.9",
    "@types/react-draft-wysiwyg": "^1.13.3",
    "@types/react-router-dom": "^5.1.8",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/parser": "^5.12.1",
    "apexcharts": "^3.33.1",
    "axios": "^0.21.1",
    "csstype": "^3.0.10",
    "d3": "^7.0.1",
    "date-fns": "^2.23.0",
    "draft-js": "^0.11.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.4.3",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "formik": "^2.2.9",
    "history": "^5.0.1",
    "i18next": "^20.4.0",
    "immutable": "^4.0.0",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^13.0.0",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-app": "^1.1.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-draft-wysiwyg": "^1.14.7",
    "react-dropzone": "^11.3.4",
    "react-helmet-async": "^1.0.9",
    "react-hot-toast": "^2.1.0",
    "react-i18next": "^11.11.4",
    "react-markdown": "^7.0.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-quill": "^1.3.5",
    "react-redux": "^7.2.4",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^4.0.3",
    "react-syntax-highlighter": "^15.4.4",
