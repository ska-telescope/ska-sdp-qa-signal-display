{
  "private": true,
  "scripts": {
    "build": "next build && next export",
    "dev": "next dev",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:eslint": "next lint --fix",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-deduplicate": "yarn install; yarn-deduplicate --strategy=fewer; yarn install",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "yarn tsc --noEmit",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "prepare": "husky install",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageDirectory=build/coverage"
  },
  "dependencies": {
