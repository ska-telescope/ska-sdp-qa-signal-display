{
  "name": "ska-webapps-react-skeleton",
  "version": "0.14.0",
  "private": true,
  "license": "BSD 3-Clause",
  "description": "SKA React Web App Skeleton",
  "main": "index.js",
  "repository": "git@gitlab.com:ska-telescope/templates/ska-react-webapp-skeleton.git",
  "scripts": {
    "build": "webpack --mode production",
    "code-analysis": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --ignore-path .eslintignore -f junit -o linting.xml",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:run:component": "cypress run --component --config video=false",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:yarn-deduplicate": "yarn install && yarn-deduplicate --strategy=fewer && yarn install",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:tsc": "yarn tsc --noEmit",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "prettier": "prettier --check \"**/*\"",
    "prettier:fix": "prettier --write \"**/*\"",
    "skao:update": "yarn add @ska-telescope/ska-gui-components@latest @ska-telescope/ska-gui-local-storage@latest",
    "start": "npx react-inject-env set -d public/ && NODE_ENV=test webpack serve --mode development --open",
    "dev": "npx react-inject-env set -d public/ && webpack serve --open --mode development",
    "test": "yarn cypress:open",
    "test:component:headless": "cypress run --component --headless --browser chrome --config video=false",
    "test:cypress:component:ci": "cypress run --component --headless --browser chrome --config video=false --config screenshotOnRunFailure=false --reporter junit --reporter-options mochaFile=build/tests/unit-tests[hash].xml",
    "test:cypress:e2e:ci": "cypress run --e2e --headless --browser chrome --reporter junit --config video=false --config screenshotOnRunFailure=false --reporter-options mochaFile=build/tests/unit-tests[hash].xml",
    "test:e2e:headless": "cypress run --e2e --headless --browser chrome --config video=false",
    "test:headless": "yarn cypress:run",
    "test:coverage:report": "npx nyc report --report-dir=build/reports --reporter=html",
    "test:coverage:report:ci": "npx nyc report --report-dir=build/reports --reporter=cobertura",
    "test:coverage:summary": "npx nyc report --reporter=text-summary",
    "merge-reports": "jrm build/reports/unit-tests.xml \"build/tests/*.xml\"",
    "test:cypress:ci": "cypress run --e2e --headless --browser chrome --config video=false --reporter junit --reporter-options mochaFile=build/tests/unit-tests[hash].xml"
  },
  "dependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.15.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@ska-telescope/ska-gui-components": "^1.0.20",
    "@ska-telescope/ska-gui-local-storage": "^1.0.1",
    "@types/react-plotly.js": "^2.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv-webpack": "^8.0.1",
    "i18next": "^21.8.9",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "json-loader": "^0.5.7",
    "junit-report-merger": "^6.0.2",
    "moment": "^2.29.4",
    "plotly.js": "^2.23.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.15.3",
    "react-plotly.js": "^2.6.0",
    "react-redux": "^8.0.5",
    "react-scripts": "^5.0.0",
    "redux": "^4.2.1",
    "typescript": "5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/runtime": "^7.17.9",
    "@cypress/code-coverage": "^3.10.7",
    "@testing-library/cypress": "10.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.33",
    "@typescript-eslint/parser": "^5.57.1",
    "babel-core": "6",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "cypress": "^13.7.3",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^3.4.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest-junit": "^16.0.0",
    "junit": "^1.4.9",
    "prettier": "^1.15.3",
    "react-inject-env": "^2.1.0",
    "sass": "^1.49.4",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/{!(i18n),}.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/index.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "author": "<karini@vivosa.co.za>"
}
