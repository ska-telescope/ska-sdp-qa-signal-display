{
  "name": "ska-webapps-react-skeleton",
  "version": "0.0.0",
  "private": true,
  "license": "BSD 3-Clause",
  "description": "SKA React Web App Skeleton",
  "main": "index.js",
  "repository": "git@gitlab.com:ska-telescope/templates/ska-react-webapp-skeleton.git",
  "scripts": {
    "build": "webpack --mode production",
    "code-analysis": "eslint src/**/*.{js,jsx.ts.tsx} --ignore-path .eslintignore -f junit -o linting.xml",
    "cypress": "cypress open",
    "lint": "eslint src/**/*.{js,jsx.ts.tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx.ts.tsx} --fix",
    "prettier": "prettier --check \"**/*\"",
    "prettier:fix": "prettier --write \"**/*\"",
    "start": "webpack serve --mode development --open"
  },
  "dependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.12.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@ska-telescope/ska-gui-components": "^0.1.19",
    "@types/react-plotly.js": "^2.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "i18next": "^21.8.9",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "json-loader": "^0.5.7",
    "moment": "^2.29.1",
    "plotly.js": "^2.23.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.15.3",
    "react-plotly.js": "^2.6.0",
    "react-redux": "^8.0.5",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^16.7.0",
    "redux": "^4.2.1",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/runtime": "^7.17.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.33",
    "@typescript-eslint/parser": "^5.57.1",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "cypress": "^12.4.0",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "html-webpack-plugin": "^5.5.0",
    "jest-junit": "^13.0.0",
    "junit": "^1.4.9",
    "prettier": "^1.15.3",
    "sass": "^1.49.4",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/{!(i18n),}.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/index.{js,jsx,ts,tsx}"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
