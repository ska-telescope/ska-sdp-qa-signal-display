{
  "name": "ska-webapps-react-skeleton",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "code-analysis": "./node_modules/.bin/eslint \"src/**\" --ignore-path .eslintignore -f junit -o linting.xml",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "lint": "eslint src/**/*.{js,jsx}",
    "test": "react-scripts test --env=jsdom --reporters=default --reporters=jest-junit",
    "test:coverage": "react-scripts test --coverage --coverageDirectory=build/coverage",
    "start": "webpack serve --open --mode development"
  },
  "dependencies": {
    "bundle-loader": "^0.5.6",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "i18next": "^21.6.6",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "jest-enzyme": "^7.0.1",
    "moment": "^2.29.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.15.3"
  },
  "devDependencies": {
    "@babel/runtime": "^7.17.9",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "html-webpack-plugin": "^5.5.0",
    "jest-junit": "^13.0.0",
    "junit": "^1.4.9",
    "prettier": "^1.15.3",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^16.7.0",
    "sass": "^1.49.4",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/{!(i18n),}.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/index.jsx"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
